{
  "name": "font-problems",
  "version": "1.1.0",
  "description": "command-line tools for font formats",
  "repository": {
    "type": "git",
    "url": "git://github.com/robey/font-problems.git"
  },
  "main": "lib/",
  "dependencies": {
    "antsy": "*",
    "lodash": "*",
    "minimist": "*",
    "source-map-support": "*",
    "sprintf": "*"
  },
  "devDependencies": {
    "babel": "*",
    "mocha": "*",
    "should": "*"
  },
  "files": [
    "lib",
    "bin",
    "fonts",
    "*.sh"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "distclean": "npm run clean && rm -rf node_modules",
    "prepublish": "babel --source-maps --out-dir lib src && babel --source-maps --out-dir test/lib test/src",
    "test": "mocha -R spec --colors test/lib"
  },
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "license": "Apache-2"
}
